<div class="modal" id="noteTimeRes" tabindex="-1" role="dialog" aria-labelledby="noteTimeResTitle"
     aria-hidden="true">

    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">

            <div class="modal-header">
                <h5>Примечание</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div id="noteTimeResinfo" class="modal-body">
                <input id="noteId" type="hidden">
                <textarea class="form-control" id="noteText"></textarea>
            </div>
            <div id="infoNote"></div>

            <div class="modal-footer">
                <div id="buttonNote" class="text-right"></div>
            </div>
        </div>
    </div>
</div>